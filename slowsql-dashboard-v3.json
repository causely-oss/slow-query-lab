          "range": true,
          "refId": "C"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          },
          "editorMode": "code",
          "expr": "anomaly:level{anomaly_name=\"slowsql_latency_p95\", db_query_text=\"$db_query_text\"} > anomaly:upper_band{anomaly_name=\"slowsql_latency_p95\", db_query_text=\"$db_query_text\"} or anomaly:level{anomaly_name=\"slowsql_latency_p95\", db_query_text=\"$db_query_text\"} < anomaly:lower_band{anomaly_name=\"slowsql_latency_p95\", db_query_text=\"$db_query_text\"}",
          "hide": false,
          "instant": false,
          "legendFormat": "anomalies",
          "range": true,
          "refId": "D"
